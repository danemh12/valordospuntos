#include <stdio.h>

int main(void) {
	int num1, num2;
	int res;
	printf("introduzaca el primer numero \n ");
	scanf("%d", &num1);
	printf("introduzca el segundo numero \n ");
	scanf("%d", &num2);
	
	if(num1>num2)
	res=num1-num2;
	else 
	res=num2-num1;
	printf("la diferencia absoluta de los numeros es: %d \n", res);
	return 0;
	
}
